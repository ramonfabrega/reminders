{
  "name": "reminders",
  "version": "1.0.1",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "prebuild": "expo prebuild --clean",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:update": "eas update -p ios --channel production",
    "ios:deploy": "eas build --platform ios --profile production --auto-submit && bun ios:metadata",
    "ios:metadata": "eas metadata:push",
    "web": "expo start --web",
    "web:build": "expo export --platform web --clear",
    "web:deploy": "bun web:build && eas deploy"
  },
  "dependencies": {
    "@expo/ui": "~0.1.1-alpha.5",
    "expo": "~53.0.0",
    "expo-application": "~6.1.3",
    "expo-constants": "~17.1.3",
    "expo-crypto": "~14.1.3",
    "expo-dev-client": "~5.1.5",
    "expo-insights": "~0.9.2",
    "expo-linking": "~7.1.3",
    "expo-router": "~5.0.3",
    "expo-sqlite": "~15.2.7",
    "expo-status-bar": "~2.2.2",
    "expo-symbols": "~0.4.3",
    "expo-updates": "~0.28.9",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.1",
    "react-native-safe-area-context": "5.3.0",
    "react-native-screens": "~4.10.0",
    "react-native-vision-camera": "^4.6.4",
    "react-native-web": "^0.20.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "eas-cli": "^16.3.3",
    "typescript": "^5.3.3"
  },
  "private": true
}
